{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "#/fusionpbx-go-api/common/schema/extension/post.json",
    "title": "extension",
    "type": "object",
    "description": "extension",
    "required": [
        "extension",
        "password",
        "enabled"
    ],
    "properties": {
        "extension": {
            "type": "string",
            "minLength": 3,
            "maxLength": 20,
            "pattern": "^[0-9]+$"
        },
        "password": {
            "type": "string",
            "minLength": 4,
            "maxLength": 200
        },
        "enabled": {
            "type": "boolean"
        },
        "user_uuid": {
            "type": "string",
            "minLength": 0,
            "maxLength": 36
        },
        "is_ring_group": {
            "type": "boolean"
        },
        "ring_group": {
            "type": "object",
            "properties": {
                "main": {
                    "type": "string",
                    "minLength": 3,
                    "pattern": "^[0-9]+$"
                },
                "sub": {
                    "type": "string",
                    "minLength": 3,
                    "pattern": "^[0-9]+$"
                }
            }
        }
    }
}