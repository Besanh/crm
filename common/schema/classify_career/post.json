{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "#/contactcenter-api/common/schema/classify_career/post.json",
    "title": "post",
    "type": "object",
    "description": "post",
    "required": [
        "career_name",
        "career_type",
        "status"
    ],
    "properties": {
        "career_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 48
        },
        "career_type": {
            "type": "string",
            "enum": [
                "career"
            ]
        },
        "status": {
            "type": "boolean",
            "default": false
        }
    }
}